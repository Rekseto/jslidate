{
    "env": {
        "browser": true,
        "es6": true
    },
    "parser": "babel-eslint",
    "extends": "eslint:recommended",
    "rules": {
        "indent": ["warn", 4],
        "quotes": ["error", "single"],
        "semi": ["error", "always"],
        "no-console": ["warn"],
        "keyword-spacing": ["error", { "after": true }],
        "space-in-parens": ["error", "never"],
        "comma-spacing": ["error", { "before": false, "after": true }]
    }
}
